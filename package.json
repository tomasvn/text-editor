{
  "name": "gulp-project",
  "version": "0.7.3",
  "description": "Gulp Project",
  "main": "index.js",
  "scripts": {
    "dev": "gulp watch || true",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint:js": "standard ./src/**/*.js",
    "lint:fix": "stylelint --fix ./src/**/*.scss && standard --fix ./src/**/*.js",
    "build": "gulp build",
    "dev:clean": "gulp clean:dev",
    "build:clean": "gulp clean",
    "deploy": "gulp deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:css --silent && npm run lint:js --silent",
      "pre-push": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasvn/gulp-project.git"
  },
  "author": "Ngoc Tu Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomasvn/gulp-project/issues"
  },
  "homepage": "https://github.com/tomasvn/gulp-project#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.1.5",
    "browser-sync": "^2.26.3",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-surge": "^0.1.0",
    "gulp-uglify": "^3.0.1",
    "gulp-useref": "^3.1.5",
    "husky": "^1.1.3",
    "run-sequence": "^2.2.1",
    "standard": "^12.0.1",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {}
}
